---
layout: nil
---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom" xml:lang="en">
    <title>Web Accessibility</title>
    <link rel="alternate"   type="text/html"            href="https://w15y.com/" />
    <link rel="self"        type="application/atom+xml" href="https://w15y.com/atom.xml" />
    <id>https://w15y.com/</id>
    <author>
        <name>Mike West</name>
        <uri>https://mikewest.org/</uri>
        <email>mike@mikewest.org</email>
    </author>
    <updated>2011-12-30T17:15:00+02:00</updated>
{% for post in site.posts limit:10 %}
    {% if forloop.first %}
    <updated>{{ post.date | date_to_xmlschema }}</updated>
    {% endif %}
    <entry>
        <title type='text'>{{ post.title }}</title>
        <link rel="alternate" href="https://w15y.com{{ post.url }}" />
        <id>https://w15y.com{{ post.id }}</id>
        <updated>{{ post.date | date_to_xmlschema }}</updated>
        <published>{{ post.date | date_to_xmlschema }}</published>
        <content type='html'>
          {{ if post.youtube }}
          &lt;iframe width="640" height="360" src="https://www.youtube-nocookie.com/embed/{{ post.youtube }}" frameborder="0" allowfullscreen&gt;&lt;/iframe&gt;
          {{ endif }}

          {{ post.content | xml_escape }}
        </content>
    </entry>
{% endfor %}
</feed>
